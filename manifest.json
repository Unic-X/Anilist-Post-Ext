{
  "name": "Anilist",
  "description": "Sends Anilist API a post request",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "service_worker": "bg.js"
  },

  "icons": {
    "128": "assets/ani.png"
  },
  "permissions": ["storage","tabs"],
  "content_scripts": [
    {
      "matches": [
        "*://animepahe.com/play*",
        "*://animepahe.org/play*",
        "*://www1.gogoanime.ai/*",
        "*://anilist.co/404"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action":{
    "default_popup":"test.html"
  }
}
